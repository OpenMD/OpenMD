//  SPC/E water model available in: J. Phys. Chem. 91, 24, (1987) 6269
//    H.J.C.Berendsen, J.R.Grigera, T.P.Straatsma

begin Options
      Name                    "sample"
      DistanceMixingRule     GEOMETRIC
      EAMMixingMethod        DREAM1
      vdW-14-scale           0.5
      electrostatic-14-scale 0.5
      MetallicEnergyUnitScaling     23.0605423
      FluctuatingChargeEnergyUnitScaling     23.0605423
end Options

begin BaseAtomTypes
//Name 	mass (amu)
C       12.0107
O       15.9994
H       1.0079
Au     	196.97
Pt      195.09
M_CO    0.0
end BaseAtomTypes

begin AtomTypes
//Name   Base	mass (amu)
OW       O
HW       H
Au_FQ    Au
Pt_FQ    Pt
O_SPCE   OW
H_SPCE   HW
CO       C
OC       O
end AtomTypes

begin LennardJonesAtomTypes
//Name  epsilon    sigma
O_SPCE  0.15532    3.16549
//Karplus and Straub model for CO
CO      0.0262     3.83
OC      0.1591     3.12
end LennardJonesAtomTypes

begin ChargeAtomTypes
//Name   charge
O_SPCE   -0.8476
H_SPCE    0.4238
//Karplus and Straub model for CO
CO       -0.75
OC       -0.85
M_CO      1.6
end ChargeAtomTypes

