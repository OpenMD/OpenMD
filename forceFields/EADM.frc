! This is the forcefield file for the Embedded Atom Method Force Field (EAM_FF).
!
! There is only a section for AtomTypes which defines mass and the name
! of the file which contains the rho(r), phi(r) and F[rho] functions.

begin BaseAtomTypes
Au	196.97
Ag	107.87
Cu	63.550
Ni	58.710
Pd	106.40
Pt	195.09
Al      26.98
Na+	100000000.00
Cl-	100000000.00
end BaseAtomTypes

begin EAMAtomTypes
Au	Au.u3.funcfl
Ag	Ag.u3.funcfl
Cu	Cu.u3.funcfl
Ni	Ni.u3.funcfl
Pd	Pd.u3.funcfl
Pt	Pt.u3.funcfl
end EAMAtomTypes


begin ChargeAtomTypes
Na+ 1.0
Cl- -1.0
end ChargeAtomTypes

